source 'https://github.com/CocoaPods/Specs.git'
source 'https://github.com/indigotech/Specs.git'

platform :ios, '8.0'

# USe frameworks for TQG installation
use_frameworks!

target :TQ1Demo do

    pod 'SimulatorRemoteNotifications', '~> 0.0.3'
    pod 'TQ1SDK', :git => 'https://github.com/tq1/taqtile-sdk-ios.git', :tag => '3.2.5-no-tracking'
    pod 'TQGeolocationSDK', :git => 'https://github.com/tq1/br-tqg-ios.git', :tag => '1.3.5'
    pod 'HockeySDK', '4.0.1'

end
